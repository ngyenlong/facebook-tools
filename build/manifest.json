{
  "short_name": "React App",
  "name": "Create React App Sample",
  "version": "1.0",
  "description": "Only using in m.facebook.com",
  "permissions": [
    "activeTab", 
    "declarativeContent", 
    "storage", 
    "tabs",
    "identity",
    "cookies",
    "http://*/*", 
    "https://*/*"
  ],
   "content_security_policy": "script-src 'self' 'sha256-jMTsFtLXMQxtXIXN+Z5L65Ee5HAILKh9a9nYE16qePU='; object-src 'self'",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "default_title": "Only using in m.facebook.com"
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
